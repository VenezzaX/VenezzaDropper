-- Scripts:

local function QSHZFIX_fake_script() -- MainTitle.LabelStuff 
	local script = Instance.new('LocalScript', MainTitle)

	local textLabel = script.Parent
	local UserInputService = game:GetService("UserInputService")
	
	textLabel.Position = UDim2.new(0.373, 0, 0, 0)
	
	local symbols = {"@", "#", "$", "%", "&", "!", "*", "^", "?", "~", "+", "-", "="}
	
	local function getRandomSymbol()
		return symbols[math.random(1, #symbols)]
	end
	
	local isShakeEnabled = false
	
	local function glitchEffect()
		local lastGlitchTime = 0
		while true do
			if isShakeEnabled then
				local randomX = math.random(-2, 2)
				local randomY = math.random(-2, 2)
				textLabel.Position = UDim2.new(0.373, randomX, 0, randomY)
			end
	
			textLabel.TextTransparency = math.random(0, 3) / 10
	
			if tick() - lastGlitchTime >= 10 then
				lastGlitchTime = tick()
	
				local originalText = textLabel.Text
				local glitchedText = ""
				local shakeDuration = math.random(0.5, 1)
				local startTime = tick()
	
				while tick() - startTime < shakeDuration do
					if isShakeEnabled then
						local randomX = math.random(-5, 5)
						local randomY = math.random(-5, 5)
						textLabel.Position = UDim2.new(0.373, randomX, 0, randomY)
					end
	
					glitchedText = ""
					for i = 1, #originalText do
						if math.random() < 0.5 then
							glitchedText = glitchedText .. getRandomSymbol()
						else
							glitchedText = glitchedText .. originalText:sub(i, i)
						end
					end
	
					textLabel.Text = glitchedText
					wait(0.05)
				end
	
				textLabel.Text = originalText
			end
	
			wait(math.random(0.1, 0.3))
		end
	end
	
	local function toggleShake(enable)
		isShakeEnabled = enable
	end
	
	UserInputService.InputBegan:Connect(function(input, gameProcessed)
		if gameProcessed then return end
		if input.KeyCode == Enum.KeyCode.N then
			isShakeEnabled = not isShakeEnabled
			print("Shake effect enabled:", isShakeEnabled)
		end
	end)
	
	glitchEffect()
end
coroutine.wrap(QSHZFIX_fake_script)()
local function XDKJO_fake_script() -- MainFrame.Night 
	local script = Instance.new('LocalScript', MainFrame)

	local InputService = game:GetService("UserInputService")
	local ContextActionService = game:GetService("ContextActionService")
	local Lighting = game:GetService("Lighting")
	
	-- Variables
	local nightVisionEnabled = false -- Tracks the state of night vision
	
	-- Helper function to toggle Night Vision
	local function toggleNightVision()
		nightVisionEnabled = not nightVisionEnabled
	
		if nightVisionEnabled then
			-- Enable Night Vision
			Lighting.Ambient = Color3.fromRGB(255, 255, 255) -- Bright lighting
		else
			-- Disable Night Vision
			Lighting.Ambient = Color3.fromRGB(0, 0, 0) -- Default/normal lighting
		end
	end
	
	-- Setup for desktop input (F key)
	if InputService.KeyboardEnabled then
		InputService.InputBegan:Connect(function(input, gameProcessed)
			if gameProcessed then return end -- Ignore input if UI or other game actions are processed
	
			if input.KeyCode == Enum.KeyCode.Q then -- Changed to F
				toggleNightVision()
			end
		end)
	end
	
	-- Setup for mobile input
	if InputService.TouchEnabled then
		ContextActionService:BindAction("NightVision", function()
			toggleNightVision()
			return Enum.ContextActionResult.Pass
		end, true, "Q") -- Kept the "f" shortcut for mobile for consistency
	
		ContextActionService:SetPosition("NightVision", UDim2.new(0.72, -25, 0.20, -25))
		ContextActionService:SetTitle("NightVision", "Night Vision")
	end
end
coroutine.wrap(XDKJO_fake_script)()
local function KPLDJJI_fake_script() -- PARASITA2.LocalScript 
	local script = Instance.new('LocalScript', PARASITA2)

	local teleportButton = script.Parent
	local teleportCoordinates = Vector3.new(-1799.1085205078125, -226.09930419921875, -1421.229248046875)
	
	teleportButton.Text = "2" 
	
	teleportButton.MouseButton1Click:Connect(function()
		local player = game.Players.LocalPlayer 
		local character = player.Character or player.CharacterAdded:Wait() 
	
		local humanoidRootPart = character:WaitForChild("HumanoidRootPart", 5)
		if humanoidRootPart then
			
			humanoidRootPart.CFrame = CFrame.new(teleportCoordinates)
			print("Teleported to: ", teleportCoordinates)
		else
			warn("HumanoidRootPart not found!")
		end
	end)
end
