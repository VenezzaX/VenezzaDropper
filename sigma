		BARCO = {position = Vector3.new(-2209.04, -0.85, -1497.05), color = Color3.fromRGB(30, 144, 255)},
		PORTAL = {position = Vector3.new(-2226.80, -174.44, -1579.84), color = Color3.fromRGB(46, 139, 87)},
		ARTEFATO_A = {position = Vector3.new(-1361.10, -456.71, -1571.41), color = Color3.fromRGB(152, 251, 152)},
		ARTEFATO_B = {position = Vector3.new(1399.90, -248.66, -1851.49), color = Color3.fromRGB(255, 248, 220)},
		ARTEFATO_C = {position = Vector3.new(-755.75, 123.14, -3165.81), color = Color3.fromRGB(70, 130, 180)},
		ARTEFATO_D = {position = Vector3.new(-1767.31, -189.90, -1294.66), color = Color3.fromRGB(72, 61, 139)},
		MARMORE_1 = {position = Vector3.new(-980.86, -2.67, -1625.13), color = Color3.fromRGB(112, 128, 144)},
		MARMORE_2 = {position = Vector3.new(-1248.98, -42.88, -1015.32), color = Color3.fromRGB(0, 255, 127)},
		MARMORE_3 = {position = Vector3.new(-1608.23, -46.68, -1557.08), color = Color3.fromRGB(255, 215, 0)},
		OLHOS_DOS_MARMORES = {position = Vector3.new(-895.14, -74.41, -1716.20), color = Color3.fromRGB(245, 222, 179)},
	}
	
	-- Player reference
	local player = game.Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	local rootPart = character:WaitForChild("HumanoidRootPart")
	
	-- Folder for waypoints in Workspace
	local folder = Instance.new("Folder")
	folder.Name = "ClientWaypoints"
	folder.Parent = workspace
	
	-- Function to create waypoints
	local function createWaypoint(name, data)
		-- Create an invisible part for positioning
		local part = Instance.new("Part")
		part.Name = name
		part.Size = Vector3.new(1, 1, 1)
		part.Position = data.position
		part.Anchored = true
		part.Transparency = 1
		part.CanCollide = false
		part.Parent = folder
	
		-- BillboardGui for the UI
		local billboard = Instance.new("BillboardGui")
		billboard.Name = "Billboard"
		billboard.Size = UDim2.new(0, 100, 0, 100)
		billboard.AlwaysOnTop = true
		billboard.StudsOffset = Vector3.new(0, 3, 0)
		billboard.Adornee = part
		billboard.Parent = part
	
		-- Name Label
		local nameLabel = Instance.new("TextLabel")
		nameLabel.Text = name
		nameLabel.Font = Enum.Font.SourceSansBold
		nameLabel.TextSize = 14
		nameLabel.TextColor3 = data.color
		nameLabel.BackgroundTransparency = 1 -- Fully transparent background
		nameLabel.Size = UDim2.new(1, 0, 0.2, 0)
		nameLabel.Position = UDim2.new(0, 0, 0.8, 0) -- Position below the center
		nameLabel.Parent = billboard
	
		return part
	end
	
	-- Create all waypoints
	for name, data in pairs(waypoints) do
		createWaypoint(name, data)
	end
	
	-- Toggle visibility
	local waypointsVisible = false
	
	local function toggleWaypointsVisibility(visible)
		for _, waypoint in pairs(folder:GetChildren()) do
			local billboard = waypoint:FindFirstChild("Billboard")
			if billboard then
				for _, guiElement in pairs(billboard:GetChildren()) do
					if guiElement:IsA("GuiObject") then
						guiElement.Visible = visible
					end
				end
			end
		end
	end
	
	-- Ensure waypoints are visible initially
	toggleWaypointsVisibility(false)
	
	-- Toggle visibility using the button
	script.Parent.MouseButton1Click:Connect(function()
		waypointsVisible = not waypointsVisible
		toggleWaypointsVisibility(waypointsVisible)
	end)
end
coroutine.wrap(WXOV_fake_script)()
local function HSGWET_fake_script() -- Countrys.LocalScript 
	local script = Instance.new('LocalScript', Countrys)

	local player = game.Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	local head = character:WaitForChild("Head")
	
	-- Wait until the LocalizationService is available
	local success, country = pcall(function()
		return game.LocalizationService:GetCountryRegionForPlayerAsync(player)
	end)
	
	local locale = player.LocaleId
	
	local playerGui = player:WaitForChild("PlayerGui")
	
	if not success then
		warn("Failed to get country region: ", country)
	end
	
	local function getFlagFromCountry(country)
		local countryToFlag = {
			-- A
			["AF"] = "🇦🇫", ["AX"] = "🇦🇽", ["AL"] = "🇦🇱", ["DZ"] = "🇩🇿", ["AS"] = "🇦🇸",
			["AD"] = "🇦🇩", ["AO"] = "🇦🇴", ["AI"] = "🇦🇮", ["AQ"] = "🇦🇶", ["AG"] = "🇦🇬",
			["AR"] = "🇦🇷", ["AM"] = "🇦🇲", ["AW"] = "🇦🇼", ["AU"] = "🇦🇺", ["AT"] = "🇦🇹",
			["AZ"] = "🇦🇿",
	
			-- B
			["BS"] = "🇧🇸", ["BH"] = "🇧🇭", ["BD"] = "🇧🇩", ["BB"] = "🇧🇧", ["BY"] = "🇧🇾",
			["BE"] = "🇧🇪", ["BZ"] = "🇧🇿", ["BJ"] = "🇧🇯", ["BM"] = "🇧🇲", ["BT"] = "🇧🇹",
			["BO"] = "🇧🇴", ["BQ"] = "🇧🇶", ["BA"] = "🇧🇦", ["BW"] = "🇧🇼", ["BV"] = "🇧🇻",
			["BR"] = "🇧🇷", ["IO"] = "🇮🇴", ["BN"] = "🇧🇳", ["BG"] = "🇧🇬", ["BF"] = "🇧🇫",
