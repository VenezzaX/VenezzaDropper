local function DOYWE_fake_script() -- SHIELDPACK_2.LocalScript 
	local script = Instance.new('LocalScript', SHIELDPACK_2)

	local teleportButton = script.Parent 
	teleportButton.Text = "X" 
	
	
	local teleportCoordinate = Vector3.new(-576.6840209960938, -173.32406616210938, -479.9155578613281)
	
	local teleportConfirmed = false 
	
	teleportButton.MouseButton1Click:Connect(function()
		
		if not teleportConfirmed then
			game:GetService("StarterGui"):SetCore("SendNotification", {
				Title = "Confirmação";
				Text = "Você tem certeza que deseja se teleportar, Você morrera se não tiver uma diving gear equipada. Clique novamente para confirmar.";
				Duration = 10;
			})
			teleportConfirmed = true
		else
			-- TP
			local player = game.Players.LocalPlayer
			local character = player.Character or player.CharacterAdded:Wait()
			local humanoidRootPart = character:WaitForChild("HumanoidRootPart", 5)
	
			if not humanoidRootPart then
				warn("HumanoidRootPart não encontrado!")
				return
			end
	
			humanoidRootPart.CFrame = CFrame.new(teleportCoordinate)
			print("Teletransportado para:", teleportCoordinate)
	
			-- WARN
			game:GetService("StarterGui"):SetCore("SendNotification", {
				Title = "Aviso";
				Text = "Você foi teleportado!";
				Icon = "rbxassetid://5430597512";
				Duration = 3;
			})
		end
	end)
end
