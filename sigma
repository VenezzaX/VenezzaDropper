			["UG"] = "ğŸ‡ºğŸ‡¬", ["UA"] = "ğŸ‡ºğŸ‡¦", ["AE"] = "ğŸ‡¦ğŸ‡ª", ["GB"] = "ğŸ‡¬ğŸ‡§", ["US"] = "ğŸ‡ºğŸ‡¸",
