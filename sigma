local function UDDEA_fake_script() -- MainFrame.Warning 
	local script = Instance.new('LocalScript', MainFrame)

	local function createWarningGui(properties)
		local player = game.Players.LocalPlayer
		local playerGui = player:WaitForChild("PlayerGui")
	
		local screenGui = Instance.new("ScreenGui")
		screenGui.Name = "WarningScreenGui"
		screenGui.Parent = playerGui
	
		local warningFrame = Instance.new("Frame")
		warningFrame.Size = properties.Size or UDim2.new(0.5, 0, 0.1, 0)
		warningFrame.Position = properties.Position or UDim2.new(0.25, 0, -0.1, 0)
		warningFrame.BackgroundColor3 = properties.BackgroundColor or Color3.fromRGB(47, 49, 54)
		warningFrame.BackgroundTransparency = properties.BackgroundTransparency or 0
		warningFrame.BorderSizePixel = properties.BorderSizePixel or 1
		warningFrame.AnchorPoint = properties.AnchorPoint or Vector2.new(0.5, 0.5)
		warningFrame.Visible = true
		warningFrame.Parent = screenGui
	
		if properties.RoundedCorners then
			local cornerRadius = Instance.new("UICorner")
			cornerRadius.CornerRadius = properties.CornerRadius or UDim.new(0, 12)
			cornerRadius.Parent = warningFrame
		end
	
		if properties.Shadow then
			local shadow = Instance.new("Frame")
			shadow.Size = UDim2.new(1, 10, 1, 10)
			shadow.Position = UDim2.new(0, -5, 0, -5)
			shadow.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
			shadow.BackgroundTransparency = 0.5
			shadow.BorderSizePixel = 0
			shadow.ZIndex = -1
			shadow.Parent = warningFrame
		end
	
		local warningLabel = Instance.new("TextLabel")
		warningLabel.Size = UDim2.new(1, 0, 1, 0)
		warningLabel.Position = UDim2.new(0, 0, 0, 0)
		warningLabel.TextColor3 = properties.TextColor or Color3.fromRGB(255, 255, 255)
		warningLabel.TextSize = properties.TextSize or 24
		warningLabel.TextStrokeTransparency = properties.TextStrokeTransparency or 0.5
		warningLabel.TextXAlignment = properties.TextXAlignment or Enum.TextXAlignment.Center
		warningLabel.TextYAlignment = properties.TextYAlignment or Enum.TextYAlignment.Center
		warningLabel.TextWrapped = properties.TextWrapped or true
		warningLabel.TextTransparency = properties.TextTransparency or 1
		warningLabel.Parent = warningFrame
	
	
		local messages = properties.Messages or {
			"Seja sensato, Seja Livre, Se Divirta.",
			"Aqueles que sabem!",
			"Eu tenho o conhecimento ğŸ˜",
			"NÃ£o sei oque escrever."
		}
		local randomMessage = messages[math.random(1, #messages)]
		warningLabel.Text = randomMessage
	
		local function fadeInLabel()
			local tweenService = game:GetService("TweenService")
			local fadeInTween = tweenService:Create(warningLabel, TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {TextTransparency = 0})
			fadeInTween:Play()
			fadeInTween.Completed:Wait()
		end
	
		local function slideInAndDisappear()
			local tweenService = game:GetService("TweenService")
	
			local slideInTween = tweenService:Create(warningFrame, TweenInfo.new(0.5, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {Position = properties.SlideInPosition or UDim2.new(0.5, 0, 0, 20)})
			slideInTween:Play()
			slideInTween.Completed:Wait()
	
			fadeInLabel()
	
			wait(properties.DisplayTime or 2)
	
			local slideOutTween = tweenService:Create(warningFrame, TweenInfo.new(0.5, Enum.EasingStyle.Back, Enum.EasingDirection.In), {Position = properties.SlideOutPosition or UDim2.new(0.5, 0, -0.2, 0)})
			slideOutTween:Play()
	
			slideOutTween.Completed:Wait()
	
			screenGui:Destroy()
		end
	
		slideInAndDisappear()
	end
	
	createWarningGui({
		Size = UDim2.new(0.5, 0, 0.1, 0),
		Position = UDim2.new(0.25, 0, -0.1, 0),
		BackgroundColor = Color3.fromRGB(47, 49, 54),
		TextColor = Color3.fromRGB(255, 255, 255),
		TextSize = 24,
		TextTransparency = 1,
		Shadow = true,
		RoundedCorners = true,
		CornerRadius = UDim.new(0, 12),
		DisplayTime = 2,
		SlideInPosition = UDim2.new(0.5, 0, 0, 20),
		SlideOutPosition = UDim2.new(0.5, 0, -0.2, 0),
		Messages = { 
			"Seja sensato, Seja Livre, Se Divirta.",
			"Aqueles que sabem!",
			"Eu tenho o conhecimento ğŸ˜",
			"NÃ£o sei oque escrever."
		}
	})
	
end
coroutine.wrap(UDDEA_fake_script)()
local function CIVG_fake_script() -- BWR.Color 
	local script = Instance.new('LocalScript', BWR)

	local TweenService = game:GetService("TweenService")
	local UserInputService = game:GetService("UserInputService")
	local button = script.Parent
	local MainFrame = button.Parent.Parent:FindFirstChild("MainFrame")
	local sub1 = MainFrame and MainFrame:FindFirstChild("sub1")
	local sub2 = MainFrame and MainFrame:FindFirstChild("sub2")
	local sub3 = MainFrame and MainFrame:FindFirstChild("sub3")
	
	if not MainFrame or not MainFrame:IsA("Frame") then
		warn("MainFrame not found or is not a Frame")
		return
	end
	
	local function validateSubframe(subframe, name)
		if not subframe or not subframe:IsA("Frame") then
			warn(name .. " not found or is not a Frame")
			return nil
		end
		return subframe
	end
	
	sub1 = validateSubframe(sub1, "sub1")
	sub2 = validateSubframe(sub2, "sub2")
	sub3 = validateSubframe(sub3, "sub3")
	
	local whiteColor = Color3.new(1, 1, 1) 
	local blackColor = Color3.new(0, 0, 0)
	local C2 = "rbxassetid://15606171585"
	local C1 = "rbxassetid://13492317832"
	
	local isWhite = false
	local isRainbow = false
	
	local tweenInfo = TweenInfo.new(
		1, 
		Enum.EasingStyle.Sine, 
		Enum.EasingDirection.InOut 
	)
	
	local function applyColorToFrames(color)
		MainFrame.BackgroundColor3 = color
		if sub1 then sub1.BackgroundColor3 = color end
		if sub2 then sub2.BackgroundColor3 = color end
		if sub3 then sub3.BackgroundColor3 = color end
	end
	
	local function toggleColor()
		if isRainbow then
			isRainbow = false 
			applyColorToFrames(blackColor)
			button.Image = C1 
			return
		end
	
		local targetColor = isWhite and blackColor or whiteColor
		local newImage = isWhite and C1 or C2
	
		TweenService:Create(MainFrame, tweenInfo, {BackgroundColor3 = targetColor}):Play()
		if sub1 then TweenService:Create(sub1, tweenInfo, {BackgroundColor3 = targetColor}):Play() end
		if sub2 then TweenService:Create(sub2, tweenInfo, {BackgroundColor3 = targetColor}):Play() end
		if sub3 then TweenService:Create(sub3, tweenInfo, {BackgroundColor3 = targetColor}):Play() end
	
		button.Image = newImage
		isWhite = not isWhite
	end
	
	local function activateRainbowMode()
		isRainbow = true
		local hue = 0
	
		local function updateRainbow()
			hue = (hue + 0.01) % 1 
			local rainbowColor = Color3.fromHSV(hue, 1, 1) 
			applyColorToFrames(rainbowColor)
		end
	
		while isRainbow do
			updateRainbow()
			wait(0.05) 
		end
	end
	
	local function toggleRainbowMode()
		if not isRainbow then
			button.Image = "rbxassetid://2866597030" 
			activateRainbowMode()
		else
			isRainbow = false -- Stop Rainbow Mode
			button.Image = "rbxassetid://13492317832" 
			applyColorToFrames(blackColor)
		end
	end
	
	button.MouseButton1Click:Connect(function()
		if not isRainbow then
			toggleColor()
		else
			isRainbow = false 
		end
	end)
	
	UserInputService.InputBegan:Connect(function(input, gameProcessed)
		if gameProcessed then
			return
		end
	
		if input.KeyCode == Enum.KeyCode.Comma then -- BIND
			toggleRainbowMode()
		end
	end)
end
coroutine.wrap(CIVG_fake_script)()
local function XPKSJC_fake_script() -- sub2.Main 
	local script = Instance.new('LocalScript', sub2)

	--// awesomegeorsome56 - free to use & 100% open source
	
	
	-- loop to roll through every set amount of time and update the text label
	while wait(0.2) do
		local player = game.Players.LocalPlayer
		local humanoidRootPart = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
	
		if humanoidRootPart then
			local velocity = humanoidRootPart.Velocity.Magnitude
			script.Parent.Speed.Text = tostring(math.round(velocity))
		end
	end
end
coroutine.wrap(XPKSJC_fake_script)()
local function FDFUJTR_fake_script() -- sub2.LocalScript 
	local script = Instance.new('LocalScript', sub2)

	local sub2 = script.Parent -- The Frame named "sub2"
	local infoLabel = sub2:FindFirstChild("Info") or Instance.new("TextLabel", sub2)
	local coordLabel = sub2:FindFirstChild("Coordinates") or Instance.new("TextLabel", sub2)
	local plr = game.Players.LocalPlayer
	
	-- Setting up the "Info" TextLabel
	infoLabel.Name = "Info"
	infoLabel.Text = "Coordenadas Atuais"
	
	-- Setting up the "Coordinates" TextLabel
	coordLabel.Name = "Coordinates"
	coordLabel.Text = "Aguardando coordenadas..."
	
	-- Updating the coordinates in real-time
	while wait() do
		if plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
			local x = math.round(plr.Character.HumanoidRootPart.CFrame.Position.X)
			local y = math.round(plr.Character.HumanoidRootPart.CFrame.Position.Y)
			local z = math.round(plr.Character.HumanoidRootPart.CFrame.Position.Z)
			coordLabel.Text = x..", "..y..", "..z
		end
	end
end
coroutine.wrap(FDFUJTR_fake_script)()
