			["AR"] = "ğŸ‡¦ğŸ‡·", ["AM"] = "ğŸ‡¦ğŸ‡²", ["AW"] = "ğŸ‡¦ğŸ‡¼", ["AU"] = "ğŸ‡¦ğŸ‡º", ["AT"] = "ğŸ‡¦ğŸ‡¹",
			["AZ"] = "ğŸ‡¦ğŸ‡¿",
	
			-- B
