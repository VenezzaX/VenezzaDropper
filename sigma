local function LFHNP_fake_script() -- sub1.Viewer 
	local script = Instance.new('LocalScript', sub1)

	local activatorButton = script.Parent:FindFirstChild("ACTIVATOR")
	
	local espEnabled = false
	local player = game.Players.LocalPlayer
	
	local function createESPForNPC(npc)
		
		if not npc:FindFirstChild("HumanoidRootPart") then
			npc:WaitForChild("HumanoidRootPart")
		end
	
		if npc == player.Character then
			return
		end
	
		-- Highlight
		local highlight = Instance.new("Highlight")
		highlight.Name = "ESPHighlight"
		highlight.FillColor = Color3.new(1, 0, 0)
		highlight.FillTransparency = 0.5
		highlight.OutlineColor = Color3.new(1, 1, 1)
		highlight.OutlineTransparency = 0
	
		highlight.Parent = npc
	end
	
	local function handleNPC(npc)
		if espEnabled then
			createESPForNPC(npc)
		end
	end
	
	local function toggleESP()
		espEnabled = not espEnabled
	
		if espEnabled then
	
			for _, npc in ipairs(game.Workspace:GetChildren()) do
				if npc:IsA("Model") and npc:FindFirstChild("HumanoidRootPart") then
					handleNPC(npc)
				end
			end
	
			game.Workspace.ChildAdded:Connect(function(child)
				if child:IsA("Model") and child:FindFirstChild("HumanoidRootPart") then
					handleNPC(child)
				end
			end)
		else
	
			for _, npc in ipairs(game.Workspace:GetChildren()) do
				if npc:IsA("Model") then
					local highlight = npc:FindFirstChild("ESPHighlight")
					if highlight then
						highlight:Destroy()
					end
				end
			end
		end
	end
	
	activatorButton.MouseButton1Click:Connect(function()
		local hasPlayedSound = false
	
	if not hasPlayedSound then
		local sound = Instance.new("Sound")
		sound.SoundId = "rbxassetid://1807494902"
		sound.Volume = 0.3
		sound.Parent = game.Workspace
		toggleESP()
		sound:Play()
		hasPlayedSound = true
	
		sound.Ended:Connect(function()
			sound:Destroy()
		
			end)
		end
	end)
end
coroutine.wrap(LFHNP_fake_script)()
local function EVGXMWV_fake_script() -- sub1.ViewerPlayer 
	local script = Instance.new('LocalScript', sub1)

	local button = script.Parent:FindFirstChild("ACTIVATOR2")
	
	local npcTag = "NPC"  
	local espEnabled = false
	local player = game.Players.LocalPlayer
	local espObjects = {}
	
	local function createESP(npc)
	
		if not npc:FindFirstChild("UpperTorso") or not npc:FindFirstChild("Head") then
			return  
		end
	
		local highlight = Instance.new("Highlight")
		highlight.Name = "ESPHighlight"
		highlight.FillColor = Color3.new(1, 1, 0.117647)
		highlight.FillTransparency = 0.5
		highlight.OutlineColor = Color3.new(0.32549, 0.0509804, 0.462745)
		highlight.OutlineTransparency = 0
		highlight.Adornee = npc.UpperTorso
		highlight.Parent = npc
	
		if not espObjects[npc] then
			local billboardGui = Instance.new("BillboardGui")
			billboardGui.Adornee = npc.Head
			billboardGui.Parent = npc
			billboardGui.Size = UDim2.new(0, 100, 0, 50)
			billboardGui.StudsOffset = Vector3.new(0, 2, 0)
			billboardGui.AlwaysOnTop = true
	
			local nameLabel = Instance.new("TextLabel")
			nameLabel.Parent = billboardGui
			nameLabel.Text = npc.Name
			nameLabel.Size = UDim2.new(1, 0, 1, 0)
			nameLabel.BackgroundTransparency = 1
			nameLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
			nameLabel.TextSize = 16
			nameLabel.TextStrokeTransparency = 0.3
	
			espObjects[npc] = billboardGui
		end
	end
	
	local function detectNPCs()
		for _, obj in pairs(workspace:GetChildren()) do
			if obj:IsA("Model") and obj:FindFirstChild("Humanoid") and obj:FindFirstChild("Head") and obj:FindFirstChild("UpperTorso") then
				if obj.Humanoid.Health > 0 then
					createESP(obj)
				end
			end
		end
	end
	
	local function disableAllESPs()
		for _, npc in pairs(workspace:GetChildren()) do
			if npc:IsA("Model") then
	
				local billboardGui = espObjects[npc]
				if billboardGui then
					billboardGui.Enabled = false
				end
	
				local highlight = npc:FindFirstChild("ESPHighlight")
				if highlight then
					highlight:Destroy()
				end
			end
		end
	end
	
	local function enableAllESPs()
		for _, npc in pairs(workspace:GetChildren()) do
			if npc:IsA("Model") then
	
				local billboardGui = espObjects[npc]
				if billboardGui then
					billboardGui.Enabled = true
				end
	
				if not npc:FindFirstChild("ESPHighlight") then
					createESP(npc)
				end
			end
		end
	end
	
	button.MouseButton1Click:Connect(function()
		espEnabled = not espEnabled
	
		local sound = Instance.new("Sound")
		sound.SoundId = "rbxassetid://462606062"
		sound.Volume = 0.3
		sound.Parent = game.Workspace
		sound:Play()
	
		sound.Ended:Connect(function()
			sound:Destroy()
		end)
	
		if espEnabled then
			detectNPCs()
			enableAllESPs()
		else
			disableAllESPs()
		end
	end)
end
coroutine.wrap(EVGXMWV_fake_script)()
