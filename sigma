		ESTUFA = {position = Vector3.new(-1341.07, 310.38, -1209.02), color = Color3.fromRGB(34, 139, 34)},
		LAIR = {position = Vector3.new(-1668.81, -13.47, -531.68), color = Color3.fromRGB(47, 79, 79)},
		ARCHIVE = {position = Vector3.new(-1955.68, -201.98, -1248.94), color = Color3.fromRGB(0, 0, 0)},
		TRANSPORTER = {position = Vector3.new(-1325.35, 111.61, -1406.87), color = Color3.fromRGB(255, 255, 255)},
