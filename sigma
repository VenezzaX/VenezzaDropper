			-- T
			["TW"] = "🇹🇼", ["TJ"] = "🇹🇯", ["TZ"] = "🇹🇿", ["TH"] = "🇹🇭", ["TL"] = "🇹🇱",
			["TG"] = "🇹🇬", ["TK"] = "🇹🇰", ["TO"] = "🇹🇴", ["TT"] = "🇹🇹", ["TN"] = "🇹🇳",
			["TR"] = "🇹🇷", ["TM"] = "🇹🇲", ["TC"] = "🇹🇨", ["TV"] = "🇹🇻",
	
			-- U
			["UG"] = "🇺🇬", ["UA"] = "🇺🇦", ["AE"] = "🇦🇪", ["GB"] = "🇬🇧", ["US"] = "🇺🇸",
			["UM"] = "🇺🇲", ["UY"] = "🇺🇾", ["UZ"] = "🇺🇿",
	
			-- V
			["VU"] = "🇻🇺", ["VE"] = "🇻🇪", ["VN"] = "🇻🇳",
	
			-- W
			["WF"] = "🇼🇫",
	
			-- Y
			["YE"] = "🇾🇪",
	
			-- Z
			["ZM"] = "🇿🇲", ["ZW"] = "🇿🇼",
		}
		return countryToFlag[country:upper()] or "❓"
	end
	
	-- Create BillboardGui for the current player
	local billboardGui = Instance.new("BillboardGui")
	billboardGui.Size = UDim2.new(0, 200, 0, 50)
	billboardGui.Adornee = head
	billboardGui.Parent = character
	billboardGui.Enabled = false
	
	billboardGui.StudsOffset = Vector3.new(0, 3, 0)
	
	local textLabel = Instance.new("TextLabel")
	textLabel.Size = UDim2.new(1, 0, 1, 0)
	textLabel.BackgroundTransparency = 1
	textLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
	textLabel.TextSize = 24
	textLabel.Font = Enum.Font.SourceSansBold
	textLabel.Text = getFlagFromCountry(country) .. " " .. locale
	textLabel.Parent = billboardGui
	
	-- Set up the button (ensure it remains functional)
	local button = script.Parent
	button.MouseButton1Click:Connect(function()
		billboardGui.Enabled = not billboardGui.Enabled
	end)
	
	-- Show the flag of other players in the same area
	game.Players.PlayerAdded:Connect(function(otherPlayer)
		if otherPlayer ~= player then
			local otherCharacter = otherPlayer.Character or otherPlayer.CharacterAdded:Wait()
			local otherHead = otherCharacter:WaitForChild("Head")
	
			-- Create BillboardGui for other players
			local otherBillboardGui = Instance.new("BillboardGui")
			otherBillboardGui.Size = UDim2.new(0, 200, 0, 50)
			otherBillboardGui.Adornee = otherHead
			otherBillboardGui.Parent = otherCharacter
			otherBillboardGui.Enabled = false
			otherBillboardGui.StudsOffset = Vector3.new(0, 3, 0)
	
			local otherTextLabel = Instance.new("TextLabel")
			otherTextLabel.Size = UDim2.new(1, 0, 1, 0)
			otherTextLabel.BackgroundTransparency = 1
			otherTextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
			otherTextLabel.TextSize = 24
			otherTextLabel.Font = Enum.Font.SourceSansBold
			local otherCountry = game.LocalizationService:GetCountryRegionForPlayerAsync(otherPlayer)
			otherTextLabel.Text = getFlagFromCountry(otherCountry) .. " " .. otherPlayer.LocaleId
			otherTextLabel.Parent = otherBillboardGui
	
			otherBillboardGui.Enabled = true
		end
	end)
	
end
coroutine.wrap(HSGWET_fake_script)()
local function PYQCYM_fake_script() -- MainFrame.Teleport 
	local script = Instance.new('LocalScript', MainFrame)

	local places = {
		Iniciais = {
			WATCHTOWER = Vector3.new(-663.6989135742188, 137.84002685546875, -287.49700927734375),
			RADIO = Vector3.new(-1053.97216796875, 505.7073669433594, -1424.8226318359375),
			OBSERVATORIO = Vector3.new(474.5674743652344, 162.9508056640625, -1216.4254150390625),
			NAVIO = Vector3.new(-385.8384094238281, 38.55583572387695, 957.79248046875),
			LOTUS = Vector3.new(-2443.72900390625, -825.2437744140625, -519.1138916015625),
			LAB = Vector3.new(-835.2711791992188, 11.608684539794922, 345.9242858886719),
			HANGAR = Vector3.new(-1626.5494384765625, 25.543996810913086, -2350.8310546875),
			DRONE = Vector3.new(-113.45889282226562, 244.67095947265625, -2795.40478515625),
			DOCAS = Vector3.new(-2022.810302734375, 5.882205486297607, -1575.8558349609375),
			BUNGALOW = Vector3.new(-292.18402099609375, -5.529104232788086, -2373.920166015625),
			ARMAZEM = Vector3.new(-853.3341674804688, 67.35806274414062, -570.5685424804688),
			MILITARYCAMP = Vector3.new(-1030.2762451171875, 266.8489685058594, -1807.54150390625)
		},
		Musica = {
			FLAUTA = Vector3.new(-1252.7984619140625, -596.82275390625, -1677.671142578125),
			VIOLAO  = Vector3.new(-381.65478515625, -20.183626174926758, 1024.6026611328125),
			BATERIA = Vector3.new(-416.5137023925781, -3.477015256881714, -2666.2578125)
		},
		Lugar = {
			GERADOR = Vector3.new(507.67333984375, 1.6812852621078491, -507.6365051269531),
			ESTUFA = Vector3.new(-1341.0689697265625, 310.38055419921875, -1209.02099609375),
			LAIR = Vector3.new(-1668.8123779296875, -13.46725082397461, -531.6766357421875),
			ARCHIVE = Vector3.new(-1955.6805419921875, -201.98011779785156, -1248.94140625),
			TRANSPORTER = Vector3.new(-1325.35302734375, 111.61395263671875, -1406.8690185546875),
			BUNKER = Vector3.new(-1374.9488525390625, 180.93661499023438, -1500.872802734375),
			FAROL = Vector3.new(-1552.9776611328125, 176.94650268554688, -198.21914672851562)
		},
		Endings = {
			BALAO = Vector3.new(-1051.505859375, 169.3422088623047, -3447.594970703125),
			AVIAO = Vector3.new(-1986.2298583984375, 205.24771118164062, -475.7407531738281),
			BARCO = Vector3.new(-2209.043701171875, -0.8520355224609375, -1497.051025390625),
			PORTAL = Vector3.new(-2226.79736328125, -174.444580078125, -1579.8431396484375)
		},
		Artefatos = {
			ARTEFATO_A = Vector3.new(-1361.09912109375, -456.7063903808594, -1571.407470703125),
			ARTEFATO_B = Vector3.new(1399.8953857421875, -248.65635681152344, -1851.4864501953125),
			ARTEFATO_C = Vector3.new(-755.7453002929688, 123.14064025878906, -3165.809326171875),
			ARTEFATO_D = Vector3.new(-1767.3140869140625, -189.89620971679688, -1294.6640625)
		},
		Marmores = {
			MARMORE_1 = Vector3.new(-980.863037109375, -2.6739470958709717, -1625.1317138671875),
			MARMORE_2 = Vector3.new(-1248.976806640625, -42.8760871887207, -1015.3152465820312),
			MARMORE_3 = Vector3.new(-1608.229736328125, -46.683082580566406, -1557.076416015625),
			OLHOS_DOS_MARMORES = Vector3.new( -895.1394653320312, -74.40823364257812, -1716.1995849609375)
		}
	}
	
	
	local function count(_table)
		local total = 0
		for _, v in pairs(_table) do
			total += 1
		end
		return total
	end
	
	local function sortTableByKey(t)
		local sortedKeys = {}
		for k in pairs(t) do
			table.insert(sortedKeys, k)
		end
		table.sort(sortedKeys)
		local sortedTable = {}
		for _, k in ipairs(sortedKeys) do
			sortedTable[k] = t[k]
		end
		return sortedTable
	end
	
	local places_frame = script.Parent.Places
	
	for category_name, category in pairs(places) do
		task.wait(0.01)
		category = sortTableByKey(category)
		
		local category_title = places_frame.title:Clone()
		category_title.Visible = true
		category_title.Text = category_name
		category_title.Parent = places_frame
	
		local category_frame = places_frame.category:Clone()
		category_frame.Size = UDim2.new(1, 0, 0, 30 * (count(category) / 4))
		category_frame.Visible = true
		category_frame.Parent = places_frame
		
		for name, pos in pairs(category) do
			local button = places_frame.layout_button:Clone()
			button.Text = name
			button.Visible = true
			button.Parent = category_frame
			
			button.MouseButton1Click:Connect(function()
				local player = game.Players.LocalPlayer 
				local character = player.Character or player.CharacterAdded:Wait() 
	
				local humanoidRootPart = character:WaitForChild("HumanoidRootPart", 5)
				if not humanoidRootPart then
					return warn("HumanoidRootPart not found!")
				end
				
				humanoidRootPart.CFrame = CFrame.new(pos)
				print("Teleported to: ", pos)
			end)
		end
	end
	
end
coroutine.wrap(PYQCYM_fake_script)()
local function CTTGW_fake_script() -- MainFrame.CloseButton 
	local script = Instance.new('LocalScript', MainFrame)

	local userInputService = game:GetService("UserInputService")
	
	local currentBind = nil
	local resetKey = Enum.KeyCode.RightBracket
	local scriptParent = script.Parent
	scriptParent.Visible = false
	
	-- (blacklist)
	local blacklistedKeys = {
		[Enum.KeyCode.Tab] = true,
		[Enum.KeyCode.Quote] = true,
		[Enum.KeyCode.Quote] = true,
		[Enum.KeyCode.CapsLock] = true,
		[Enum.KeyCode.LeftShift] = true,
		[Enum.KeyCode.RightShift] = true,
		[Enum.KeyCode.LeftControl] = true,
		[Enum.KeyCode.RightControl] = true,
		[Enum.KeyCode.W] = true,
		[Enum.KeyCode.A] = true,
		[Enum.KeyCode.S] = true,
		[Enum.KeyCode.D] = true,
		[Enum.KeyCode.M] = true,
		[Enum.KeyCode.LeftBracket] = true,
		[Enum.KeyCode.Space] = true,
		[Enum.KeyCode.Escape] = true,
		[Enum.KeyCode.Backspace] = true
	}
	
	local screenGui = Instance.new("ScreenGui")
	screenGui.Name = "BindPromptGui"
	screenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
	
	local frame = Instance.new("Frame")
	frame.Size = UDim2.new(0.3, 0, 0.2, 0)
	frame.Position = UDim2.new(0.5, 0, 0.5, 0)
	frame.AnchorPoint = Vector2.new(0.5, 0.5)
	frame.BackgroundColor3 = Color3.new(0.2, 0.2, 0.2)
	frame.BorderSizePixel = 0
	frame.Parent = screenGui
	
	local textLabel = Instance.new("TextLabel")
	textLabel.Size = UDim2.new(1, 0, 0.6, 0)
	textLabel.Position = UDim2.new(0, 0, 0.2, 0)
	textLabel.Text = "Escolha qualquer tecla (menos o mouse ou qualquer tecla que envolve a jogabilidade) para ser sua BIND"
	textLabel.TextScaled = true
	textLabel.TextColor3 = Color3.new(1, 1, 1)
	textLabel.BackgroundTransparency = 1
	textLabel.Parent = frame
	
	-- Adicionando o som
	local sound = Instance.new("Sound")
	sound.SoundId = "rbxassetid://1655262564"
	sound.Volume = 1
	sound.Looped = false
	sound.Parent = screenGui -- O som será tocado no Gui
	
	local function promptForBind()
		frame.Visible = true
		local connection
		connection = userInputService.InputBegan:Connect(function(input, isProcessed)
			if isProcessed then return end
	
			if input.UserInputType == Enum.UserInputType.Keyboard then
	
				if input.KeyCode == Enum.KeyCode.Quote then
					textLabel.Text = "A tecla ' não pode ser usada pois ela serve para abrir o inventário no Roblox."
					wait(1.5)
					textLabel.Text = "Escolha qualquer tecla (menos o mouse ou qualquer tecla que envolve a jogabilidade) para ser sua BIND"
					return
				end
	
				if blacklistedKeys[input.KeyCode] then
					textLabel.Text = "Essa tecla é necessária para jogar e não pode ser usada."
					wait(1.5)
					textLabel.Text = "Escolha qualquer tecla (menos o mouse ou qualquer tecla que envolve a jogabilidade) para ser sua BIND"
					return
				end
	
				currentBind = input.KeyCode
				print("Nova tecla é: " .. tostring(currentBind))
	
				game:GetService("StarterGui"):SetCore("SendNotification", {
					Title = "Aviso";
					Text = "Menu Carregado. Pressione " .. tostring(input.KeyCode.Name) .. " como sua BIND";
					Icon = "rbxassetid://16541142603";
				})
				
				-- Tocar o som
				sound:Play()
	
				frame.Visible = false
				scriptParent.Visible = true
	
				connection:Disconnect()
			else
				textLabel.Text = "Escolha uma tecla do seu teclado"
				wait(1)
				textLabel.Text = "Escolha qualquer tecla (menos o mouse) para ser sua BIND"
			end
		end)
	end
	
	promptForBind()
	
	userInputService.InputBegan:Connect(function(input, isProcessed)
		if isProcessed then return end
	
		if currentBind and input.KeyCode == currentBind then
			scriptParent.Visible = not scriptParent.Visible
		elseif input.KeyCode == resetKey then
			print("Resetando a tecla BIND...")
			scriptParent.Visible = false
			currentBind = nil
			promptForBind()
		end
	end)
end
coroutine.wrap(CTTGW_fake_script)()
local function FLIS_fake_script() -- APECITY.LocalScript 
	local script = Instance.new('LocalScript', APECITY)

	local teleportButton = script.Parent
	teleportButton.Text = "X"
	
	local teleportCoordinates = {
		Vector3.new(-354.64971923828125, -38.69834899902344, 1171.4146728515625),
		Vector3.new(766.4276123046875, -17.989133834838867, -1117.763671875),
		Vector3.new(-685.798095703125, 149.46075439453125, -1052.0863037109375),
		Vector3.new(-1581.9991455078125, -6.79750394821167, 999.5809326171875),
		Vector3.new(-2225.726806640625, -3.4310882091522217, -852.5384521484375)
	}
	
	teleportButton.MouseButton1Click:Connect(function()
		
		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "Aviso";
			Text = "Isso pode levar varias tentativas se mexa bastante";
			Icon = "rbxassetid://17432832531";
		})
		
		local player = game.Players.LocalPlayer 
		local character = player.Character or player.CharacterAdded:Wait()
		local humanoidRootPart = character:WaitForChild("HumanoidRootPart", 5) 
	
		if not humanoidRootPart then
			warn("HumanoidRootPart not found!")
			return
		end
	
		for _, coords in ipairs(teleportCoordinates) do
			humanoidRootPart.CFrame = CFrame.new(coords)
			print("Teleported to: ", coords)
			wait(0.5)
		end
	end)
end
coroutine.wrap(FLIS_fake_script)()
local function AGHZRI_fake_script() -- VEST.LocalScript 
	local script = Instance.new('LocalScript', VEST)

	local teleportButton = script.Parent 
	teleportButton.Text = "X"
	
	local teleportCoordinates = {
		Vector3.new(-715.4263305664062, -6.315384387969971, 870.6802978515625),
		Vector3.new(-1955.7611083984375, -203.9578857421875, -1264.7694091796875),
	}
	
	teleportButton.MouseButton1Click:Connect(function()
		
		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "Aviso";
			Text = "Aperte a letra E estiver sendo teleportado";
		})
		
		local player = game.Players.LocalPlayer
		local character = player.Character or player.CharacterAdded:Wait()
		local humanoidRootPart = character:WaitForChild("HumanoidRootPart", 5)
	
		if not humanoidRootPart then
			warn("HumanoidRootPart not found!")
			return
		end
	
		for _, coords in ipairs(teleportCoordinates) do
			humanoidRootPart.CFrame = CFrame.new(coords)
			print("Teleported to: ", coords)
			wait(1.5)
		end
	end)
end
